{
  "name": "gemini-feedic",
  "version": "1.0.0",
  "description": "RSS Reader with auto-refresh development server",
  "main": "server.js",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch . --ext js,html,css --ignore node_modules server.js",
    "dev:simple": "nodemon server.js",
    "clean": "rm -rf node_modules pnpm-lock.yaml",
    "reinstall": "pnpm clean && pnpm install"
  },
  "keywords": ["rss", "reader", "feedic"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
