{
  "name": "gemini-feedic",
  "version": "1.7.0",
  "description": "RSS Reader with auto-refresh development server",
  "main": "server.js",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch . --ext js,html,css --ignore node_modules server.js",
    "dev:simple": "nodemon server.js",
    "clean": "rm -rf node_modules pnpm-lock.yaml",
    "reinstall": "pnpm clean && pnpm install",
    "prepare": "husky",
    "version": "standard-version",
    "release": "standard-version && git push --follow-tags"
  },
  "keywords": [
    "rss",
    "reader",
    "feedic"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "standard-version": "^9.5.0"
  }
}
